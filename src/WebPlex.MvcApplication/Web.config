<?xml version="1.0"
      encoding="utf-8" ?>

<configuration>
	<configSections>
		<section name="plexConfig"
		         type="WebPlex.Core.Configuration.PlexConfig" />
		<section name="bootstrapConfig"
		         type="WebPlex.Core.Configuration.BootstrapConfig" />
		<section name="oAuthConfig"
